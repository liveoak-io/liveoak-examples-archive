{ 
    "uriRules": [
        {
            "priority": 1,
            "uriPattern": "/auth",
            "requestType": "READ",
            "allowedUsers": "'*'"
        },
        {
            "priority": 1,
            "uriPattern": "/auth/app/index.html",
            "requestType": "READ",
            "allowedUsers": "'*'"
        },
        {
            "priority": 1,
            "uriPattern": "/auth/client*",
            "requestType": "*",
            "allowedUsers": "'*'"
        },
        {
            "priority": 1,
            "uriPattern": "/auth/storage*",
            "requestType": "READ",
            "allowedRoles": "'test-app/user'"
        }
    ]
}
