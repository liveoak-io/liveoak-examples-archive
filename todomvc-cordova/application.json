{
  resources: {
    client: {
      type: 'aggregating-filesystem',
      config: {
        directory: "${io.liveoak.js.dir}"
      }
    },

    css: {
      type: 'aggregating-filesystem',
      config: {
        directory: "${io.liveoak.css.dir}"
      }
    },

    auth: {
      type: 'keycloak'
    },

    authz: {
      type: 'security'
    },

    aclPolicy: {
      type: 'aclPolicy'
    },

    uriPolicy: {
      type: 'uriPolicy'
    },

    storage: {
      type: 'mongo',
      config: {
        db: 'liveoak',
        servers: [
          {
            host: 'localhost',
            port: 27017
          }
        ]
      }
    }

  }
}
